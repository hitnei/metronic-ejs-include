<div class="kt-portlet kt-portlet--fit kt-portlet--head-noborder kt-portlet--height-fluid-half">
    <div class="kt-portlet__head kt-portlet__space-x">
        <div class="kt-portlet__head-label">
            <h3 class="kt-portlet__head-title">
                <%= data.portletSection1.head.title %>
            </h3>
        </div>
        <div class="kt-portlet__head-toolbar">
            <a href="#" class="btn btn-label-success btn-sm btn-bold dropdown-toggle" data-toggle="dropdown">
                <%= data.portletSection1.head.dropdown.btn %>
            </a>
            <div class="dropdown-menu dropdown-menu-fit dropdown-menu-right">
                <ul class="kt-nav">
                    <li class="kt-nav__item">
                        <a href=<%= data.portletSection1.head.dropdown.link[0] %> class="kt-nav__link">
                            <i class="kt-nav__link-icon flaticon2-line-chart"></i>
                            <span class="kt-nav__link-text"><%= data.portletSection1.head.dropdown.text[0] %></span>
                        </a>
                    </li>
                    <li class="kt-nav__item">
                        <a href=<%= data.portletSection1.head.dropdown.link[1] %> class="kt-nav__link">
                            <i class="kt-nav__link-icon flaticon2-send"></i>
                            <span class="kt-nav__link-text"><%= data.portletSection1.head.dropdown.text[1] %></span>
                        </a>
                    </li>
                    <li class="kt-nav__item">
                        <a href=<%= data.portletSection1.head.dropdown.link[2] %> class="kt-nav__link">
                            <i class="kt-nav__link-icon flaticon2-pie-chart-1"></i>
                            <span class="kt-nav__link-text"><%= data.portletSection1.head.dropdown.text[2] %></span>
                        </a>
                    </li>
                    <li class="kt-nav__item">
                        <a href=<%= data.portletSection1.head.dropdown.link[3] %> class="kt-nav__link">
                            <i class="kt-nav__link-icon flaticon2-avatar"></i>
                            <span class="kt-nav__link-text"><%= data.portletSection1.head.dropdown.text[3] %></span>
                        </a>
                    </li>
                    <li class="kt-nav__item">
                        <a href=<%= data.portletSection1.head.dropdown.link[4] %> class="kt-nav__link">
                            <i class="kt-nav__link-icon flaticon2-settings"></i>
                            <span class="kt-nav__link-text"><%= data.portletSection1.head.dropdown.text[4] %></span>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
    <div class="kt-portlet__body kt-portlet__body--fluid">
        <div class="kt-widget20">
            <div class="kt-widget20__content kt-portlet__space-x">
                <span class="kt-widget20__number kt-font-brand"><%= data.portletSection1.body.number %></span>
                <span class="kt-widget20__desc"><%= data.portletSection1.body.desc %></span>
            </div>
            <div class="kt-widget20__chart" style="height:130px;">
                <canvas id="kt_chart_bandwidth1"></canvas>
            </div>
        </div>
    </div>
</div>