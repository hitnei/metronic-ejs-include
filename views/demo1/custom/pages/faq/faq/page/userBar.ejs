
<div class="kt-header__topbar-item kt-header__topbar-item--user">
	<div class="kt-header__topbar-wrapper" data-toggle="dropdown" data-offset="0px,0px">
		<div class="kt-header__topbar-user">
			<span class="kt-header__topbar-welcome kt-hidden-mobile"><%= data.userBar.user.welcome %></span>
			<span class="kt-header__topbar-username kt-hidden-mobile"><%= data.userBar.user.username %></span>
			<img class="kt-hidden" alt="Pic" src=<%= data.userBar.user.img %> />

			<!--use below badge element instead the user avatar to display username's first letter(remove kt-hidden class to display it) -->
			<span class="kt-badge kt-badge--username kt-badge--unified-success kt-badge--lg kt-badge--rounded kt-badge--bold"><%= data.userBar.user.badge %></span>
		</div>
	</div>
	<div class="dropdown-menu dropdown-menu-fit dropdown-menu-right dropdown-menu-anim dropdown-menu-top-unround dropdown-menu-xl">

		<!--begin: Head -->
		<div class="kt-user-card kt-user-card--skin-dark kt-notification-item-padding-x" style="background-image: url(/assets/media/misc/bg-1.jpg)">
			<div class="kt-user-card__avatar">
				<img class="kt-hidden" alt="Pic" src=<%= data.userBar.head.img %> />

				<!--use below badge element instead the user avatar to display username's first letter(remove kt-hidden class to display it) -->
				<span class="kt-badge kt-badge--lg kt-badge--rounded kt-badge--bold kt-font-success"><%= data.userBar.head.badge %></span>
			</div>
			<div class="kt-user-card__name">
				<%= data.userBar.head.cardName %>
			</div>
			<div class="kt-user-card__badge">
				<span class="btn btn-success btn-sm btn-bold btn-font-md"><%= data.userBar.head.cardBadge %></span>
			</div>
		</div>

		<!--end: Head -->

		<!--begin: Navigation -->
		<div class="kt-notification">
			<a href="#" class="kt-notification__item">
				<div class="kt-notification__item-icon">
					<i class="flaticon2-calendar-3 kt-font-success"></i>
				</div>
				<div class="kt-notification__item-details">
					<div class="kt-notification__item-title kt-font-bold">
						<%= data.userBar.navigation.title[0] %>
					</div>
					<div class="kt-notification__item-time">
						<%= data.userBar.navigation.time[0] %>
					</div>
				</div>
			</a>
			<a href="#" class="kt-notification__item">
				<div class="kt-notification__item-icon">
					<i class="flaticon2-mail kt-font-warning"></i>
				</div>
				<div class="kt-notification__item-details">
					<div class="kt-notification__item-title kt-font-bold">
						<%= data.userBar.navigation.title[1] %>
					</div>
					<div class="kt-notification__item-time">
						<%= data.userBar.navigation.time[1] %>
					</div>
				</div>
			</a>
			<a href="#" class="kt-notification__item">
				<div class="kt-notification__item-icon">
					<i class="flaticon2-rocket-1 kt-font-danger"></i>
				</div>
				<div class="kt-notification__item-details">
					<div class="kt-notification__item-title kt-font-bold">
						<%= data.userBar.navigation.title[2] %>
					</div>
					<div class="kt-notification__item-time">
						<%= data.userBar.navigation.time[2] %>
					</div>
				</div>
			</a>
			<a href="#" class="kt-notification__item">
				<div class="kt-notification__item-icon">
					<i class="flaticon2-hourglass kt-font-brand"></i>
				</div>
				<div class="kt-notification__item-details">
					<div class="kt-notification__item-title kt-font-bold">
						<%= data.userBar.navigation.title[3] %>
					</div>
					<div class="kt-notification__item-time">
						<%= data.userBar.navigation.time[3] %>
					</div>
				</div>
			</a>
			<a href="#" class="kt-notification__item">
				<div class="kt-notification__item-icon">
					<i class="flaticon2-cardiogram kt-font-warning"></i>
				</div>
				<div class="kt-notification__item-details">
					<div class="kt-notification__item-title kt-font-bold">
						<%= data.userBar.navigation.title[4] %>
					</div>
					<div class="kt-notification__item-time">
						<%= data.userBar.navigation.time[4] %>
						<span class="kt-badge kt-badge--danger kt-badge--inline kt-badge--pill kt-badge--rounded"><%= data.userBar.navigation.badge %></span>
					</div>
				</div>
			</a>
			<div class="kt-notification__custom kt-space-between">
				<a href=<%= data.userBar.navigation.link[0] %> target="_blank" class="btn btn-label btn-label-brand btn-sm btn-bold"><%= data.userBar.navigation.btn[0] %></a>
				<a href=<%= data.userBar.navigation.link[1] %> target="_blank" class="btn btn-clean btn-sm btn-bold"><%= data.userBar.navigation.btn[1] %></a>
			</div>
		</div>

		<!--end: Navigation -->
	</div>
</div>